{
    "extends": ["tslint:recommended",  "tslint-config-prettier"],
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "jsdoc-format": false,
        "max-classes-per-file": {
            "options": [1]
        },
        "comment-format": false,
        "no-empty": false,
        "array-type": [true, "array-simple"]
    }
}
